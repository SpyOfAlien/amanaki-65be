    },
    "Thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Price": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "Details": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "section.room-special-requirement",
        "section.simple-info",
        "symmetry.images-grid",
        "symmetry.images-swipeable",
        "symmetry.simple",
        "ui.amenity",
        "ui.button"
      ]
    },
    "Slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "Name"
    },
    "SpecialRequirement": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "section.room-special-requirement"
    },
    "Overview": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "Rooms": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::room-list.room-list"
    },
    "BannerBackground": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
